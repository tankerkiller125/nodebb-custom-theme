{
  "name": "nodebb-theme-homebox",
  "version": "2.2.7",
  "nbbpm": {
    "compatibility": "^4.0.0"
  },
  "description": "Homebox theme for NodeBB",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tankerkiller125/nodebb-custom-theme"
  },
  "scripts": {
    "lint": "eslint .",
    "build:css": "tailwindcss -i tailwind.css -o public/tailwind.css --minify",
    "prepublishOnly": "npm run build:css",
    "publish:patch": "npm version patch && npm run publish:common",
    "publish:minor": "npm version minor && npm run publish:common",
    "publish:major": "npm version major && npm run publish:common",
    "publish:common": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "nodebb",
    "theme",
    "forum",
    "tailwindcss",
    "responsive"
  ],
  "contributors": [
    {
      "name": "Julian Lam",
      "email": "julian@nodebb.org",
      "url": "https://github.com/julianlam"
    },
    {
      "name": "Barış Soner Uşaklı",
      "email": "baris@nodebb.org",
      "url": "https://github.com/barisusakli"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tankerkiller125/nodebb-custom-theme/issues"
  },
  "dependencies": {
    "@fontsource/inter": "5.2.8",
    "@fontsource/poppins": "5.2.7",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "eslint": "10.0.0",
    "eslint-config-nodebb": "^1.1.4"
  }
}
